# Example Terraform variables file
# Copy this to terraform.tfvars and modify as needed

# AWS region
aws_region = "ap-northeast-1"

# Environment (dev, staging, prod)
environment = "dev"

# Project name
project_name = "futura"

# Optional: Custom S3 bucket name for uploads (must be globally unique)
# If not specified, defaults to: {project_name}-{environment}-uploads
# Uncomment and customize if you encounter bucket name conflicts:
# uploads_bucket_name = "your-company-futura-dev-uploads-unique-identifier"

# Optional: Custom Cognito User Pool Domain (must be unique within the AWS region)
# If not specified, defaults to: {project_name}-{environment}-auth
# Uncomment and customize if you encounter domain conflicts:
# cognito_domain = "your-company-futura-dev-auth"